\n        message: responseEnv.message\r\n      });      \r\n    }else{\r\n      setResponse({\r\n        formSave: false,\r\n        type: 'success',\r\n        message: responseEnv.message\r\n      });       \r\n    }\r\n  }catch(err){\r\n      setResponse({\r\n        formSave: false,\r\n        type: 'error',\r\n        message: \"Erro: Orçamento não enviado!\"\r\n      }); \r\n  }\r\n \r\n}\r\n\r\n    return(\r\n      <div>\r\n        <Head>\r\n            <title>Orçamento - Celke</title>\r\n            <meta name=\"description\" content=\"Página de orçamento realizada durante a imersão FullStack Celke\"></meta>\r\n        </Head>\r\n\r\n        <Menu />\r\n        \r\n        <Jumbotron fluid className=\"descr-top\">\r\n        <style>\r\n            {`.descr-top{\r\n                background-color: #050c3d;\r\n                color: #00a1fc;\r\n                padding-top: 40px;\r\n                padding-bottom: 40px;\r\n                margin-bottom: 0rem !important;\r\n            }`}\r\n        </style>\r\n        <Container className=\"text-center\">\r\n          <h1 className=\"display-4\">Orçamento</h1>\r\n        </Container>\r\n        </Jumbotron>\r\n\r\n        <Jumbotron fluid className=\"form-orcamento\">\r\n        <style>\r\n            {`.form-orcamento{\r\n                padding-top: 80px;\r\n                padding-bottom: 40px;\r\n                background-color: #fff;                 \r\n                margin-bottom: 0rem !important;\r\n            }`}\r\n        </style>\r\n        <Container>\r\n\r\n          {response.type === 'error' ? \r\n            <Alert color=\"danger\">{response.message}</Alert>: \"\"\r\n          }\r\n          {response.type === 'success' ? \r\n            <Alert color=\"success\">{response.message}</Alert>: \"\"\r\n          }\r\n\r\n          <Form onSubmit={sendOrcamento}>\r\n            <FormGroup>\r\n              <Label for=\"name\">Nome</Label>\r\n              <Input type=\"text\" name=\"name\" \r\n                 id=\"name\" \r\n                 placeholder=\"Preencha com o nome completo:\" \r\n                 onChange={onChangeInput}\r\n              />\r\n            </FormGroup>\r\n            <FormGroup>\r\n              <Label for=\"email\">E-mail</Label>\r\n              <Input type=\"email\" \r\n                 name=\"email\" id=\"email\" \r\n                 placeholder=\"Informe o seu principal E-mail:\" \r\n                 onChange={onChangeInput}\r\n              />\r\n            </FormGroup>\r\n            <FormGroup>\r\n              <Label for=\"phone\">Telefone</Label>\r\n              <Input type=\"text\" \r\n                 name=\"phone\" id=\"phone\" \r\n                 placeholder=\"(xx) xxxx-xxxx\" \r\n                 onChange={onChangeInput}\r\n              />\r\n            </FormGroup>\r\n            <FormGroup>\r\n              <Label for=\"whatsApp\">WhatsApp</Label>\r\n              <Input type=\"text\" \r\n                 name=\"whatsApp\" id=\"whatsApp\" \r\n                 placeholder=\"(xx) xxxxx-xxxx\" \r\n                 onChange={onChangeInput}\r\n              />\r\n            </FormGroup>\r\n            <FormGroup>\r\n              <Label for=\"projeto\">Projeto</Label>\r\n              <Input type=\"textarea\" \r\n                 name=\"projeto\" id=\"projeto\" \r\n                 placeholder=\"Fale sobre o seu projeto :\" \r\n                 onChange={onChangeInput}\r\n              />\r\n            </FormGroup>\r\n\r\n            {response.formSave ? \r\n            <Button type=\"submit\" outline color=\"dark\" disabled>Enviando...</Button>: \r\n            <Button type=\"submit\" outline color=\"primary\">Solicitar</Button>\r\n            }\r\n            \r\n          </Form>          \r\n        </Container>\r\n\r\n        </Jumbotron>\r\n\r\n        <Rodape />\r\n    </div>\r\n    )\r\n  }\r\n  \r\n  export default Orcamento;\r\n  "]},"metadata":{},"sourceType":"module"}